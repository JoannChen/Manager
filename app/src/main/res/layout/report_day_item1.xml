<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    style="@style/Layout_Wrap_Vertical_White_Style">

    <!--日报表-子条目-外层列表-->


    <!--收费平台及收入-->

    <TextView
        android:id="@id/tv_price"
        android:layout_width="match_parent"
        android:layout_height="@dimen/px78"
        android:layout_marginStart="@dimen/px62"
        android:gravity="center_vertical"
        android:text="@string/app_text"
        android:textColor="@color/black_font"
        android:textSize="@dimen/px30" />

    <!--分割线-->

    <View style="@style/View_Line_Style" />

    <!--存放列表的容器-->

    <LinearLayout
        android:id="@+id/ll_container2"
        style="@style/Layout_Wrap_Vertical_White_Style" />

    <!--分割色块 -->

    <View style="@style/View_Division_Style" />

</LinearLayout>